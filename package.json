{
  "name": "crowd-vc-monorepo",
  "version": "2.9.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rimraf -g \"node_modules\"",
    "web:dev": "turbo run dev --filter=@crowd-vc/web",
    "web:build": "turbo run build --filter=@crowd-vc/web",
    "compile": "turbo run compile --filter=@crowd-vc/contracts",
    "contracts:test": "turbo run test --filter=@crowd-vc/contracts",
    "deploy": "turbo run deploy --filter=@crowd-vc/contracts",
    "db:generate": "cd apps/web && pnpm db:generate",
    "db:migrate": "cd apps/web && pnpm db:migrate",
    "db:push": "cd apps/web && pnpm db:push",
    "db:studio": "cd apps/web && pnpm db:studio",
    "db:seed": "cd apps/web && pnpm db:seed",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20.16.0",
    "pnpm": ">=10.0.0"
  }
}